[Version]
Signature=$CHICAGO$
			; $Windows95$	; 16-битная версия Windows
			; $WindowsNT$	; 32-битная версия Windows

[DefaultInstall]
BitReg=Bits.Set
AddReg=Reg.Settings
DelReg=Reg.Uninstall
CopyFiles=Inf.Copy

[Reg.Settings]
;ROOT, SUBKEY[, NAME[, FLAG[, DATA]]]
;
;FLAG :
;
; 0x00000 - REG_SZ
; 0x00001 - REG_BINARY
; 0x10000 - REG_MULTI_SZ
; 0x20000 - REG_EXPAND_SZ
; 0x10001 - REG_DWORD
; 0x20001 - REG_NONE

[Bits.Set]
;ROOT, SUBKEY, NAME, FLAG, MASK, BYTE
;
;FLAG :
;
; 0x00000 - сброс разрядов в маске
; 0x00001 - установка разрядов в маске

[Reg.Uninstall]
HKCU,Software\Microsoft\Windows\CurrentVersion\Uninstall\%NAME%
HKCU,Software\Microsoft\Windows\CurrentVersion\Uninstall\%NAME%, DisplayName, , "%NAME%"
HKCU,Software\Microsoft\Windows\CurrentVersion\Uninstall\%NAME%, UninstallString\, , "Rundll32.exe setupapi.dll, InstallHinfSection DefaultUninstall 132 %53%\Application Data\Custom\Имя файла.inf"

;ROOT :
;
;HKCU
;HKLM

[Inf.Copy]
Имя файла.inf

[DestinationDirs]
Inf.Copy = 53, Application Data\Custom

; Индетификаторы каталогов
; 10 - %SystemRoot%
; 11 - %SystemRoot%\System32
; 17 - %SystemRoot%\Inf
; 53 - %UserProfile%
; -1 - Абсолютный путь

[SourceDiskNames]
55=%DISKNAME%

[SourceDiskFiles]
Имя файла.inf=55

[Strings]
NAME         = "Имя файла"
OWNER        = "Имя пользователя"
DISKNAME     = "Setup Files"
HK_DESKTOP   = "Control Panel\Desktop"
HK_EXPLORER  = "Software\Microsoft\Windows\CurrentVersion\Explorer"
HK_SEARCH    = "Software\Microsoft\Search Assistant"
HK_INTERNET  = "Software\Microsoft\Internet Explorer"
HK_SETUP     = "SOFTWARE\Microsoft\Windows NT\CurrentVersion"
